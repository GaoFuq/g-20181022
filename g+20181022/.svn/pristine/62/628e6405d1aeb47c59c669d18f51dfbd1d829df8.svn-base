/***
 * 热门直播每一个item
 */
public class HotLIveLists {
    String nikeName;
    int imageId;
    String watchNumber;
    int watchIcon;

    public HotLIveLists() {

    }

    public HotLIveLists(String nikeName, int imageId, String watchNumber, int watchIcon) {
        this.nikeName = nikeName;
        this.imageId = imageId;
        this.watchNumber = watchNumber;
        this.watchIcon = watchIcon;
    }

    public String getNikeName() {
        return nikeName;
    }

    public void setNikeName(String nikeName) {
        this.nikeName = nikeName;
    }

    public int getImageId() {
        return imageId;
    }

    public void setImageId(int imageId) {
        this.imageId = imageId;
    }

    public String getWatchNumber() {
        return watchNumber;
    }

    public void setWatchNumber(String watchNumber) {
        this.watchNumber = watchNumber;
    }

    public int getWatchIcon() {
        return watchIcon;
    }

    public void setWatchIcon(int watchIcon) {
        this.watchIcon = watchIcon;
    }
}
